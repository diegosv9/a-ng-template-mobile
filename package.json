{
  "name": "a-ng-template",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:e2e": "ng e2e",
    "test:e2e:report": "npx playwright show-report",
    "lint": "ng lint",
    "capacitor:sync": "ng build && npx cap sync",
    "capacitor:sync:android": "ng build && npx cap sync android",
    "capacitor:sync:ios": "ng build && npx cap sync ios",
    "capacitor:assets:generate": "npx capacitor-assets generate",
    "capacitor:assets:generate:android": "npx capacitor-assets generate android",
    "capacitor:assets:generate:ios": "npx capacitor-assets generate ios",
    "prettier:check": "prettier --check src/**/*.ts src/**/*.html src/**/*.css",
    "prettier:write": "prettier --write src/**/*.ts src/**/*.html src/**/*.css",
    "husky:prepare": "husky",
    "husky:pre-commit": "npm run lint && npm run prettier:check",
    "husky:pre-push": "npm run test && npm run test:e2e",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "generate:api": "npx orval --config orval.config.ts && prettier --write src/generated/**/*.ts && prettier --write src/environments/**/*.ts",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.10",
    "@angular/compiler": "^19.2.10",
    "@angular/core": "^19.2.10",
    "@angular/forms": "^19.2.10",
    "@angular/platform-browser": "^19.2.10",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/router": "^19.2.10",
    "@capacitor/android": "^7.2.0",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/ios": "^7.2.0",
    "@jsverse/transloco": "^7.6.1",
    "@ng-icons/core": "^31.3.0",
    "@ng-icons/lucide": "^31.3.0",
    "@primeng/themes": "^19.1.2",
    "@tailwindcss/postcss": "^4.1.5",
    "postcss": "^8.5.3",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.2",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.5",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.10",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.10",
    "@capacitor/assets": "^3.0.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jsverse/transloco-keys-manager": "^6.1.0",
    "@playwright/test": "1.52.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "angular-eslint": "19.4.0",
    "eslint": "^9.26.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "ngx-build-plus": "^19.0.0",
    "orval": "^7.9.0",
    "playwright-ng-schematics": "^2.0.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.32.1"
  }
}
